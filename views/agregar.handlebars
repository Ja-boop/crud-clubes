<div class="borde" id="container-form-agregar">
    <form method="POST" target="/equipos/agregar">
        
        <div class="form-group">
            <label for="nombre">Nombre</label>
            <input name="nombre" type="text" class="form-control" placeholder="Ingresa el nombre del equipo" autofocus required>
        </div>

        <div class="form-group">
            <label for="pais">Pais</label>
            <input name="pais" type="text" class="form-control" placeholder="Ingresa el pais del equipo" autofocus required>
        </div>

    </form>
</div>
